<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>omega</display-name>

    <servlet>
        <servlet-name>PatientServlet</servlet-name>
        <servlet-class>com.labclinico.omega.servlet.PatientServlet</servlet-class>
        <servlet-name>DoctoresServlet</servlet-name>
        <servlet-class>com.labclinico.omega.servlet.DoctoresServlet</servlet-class>
        <servlet-name>CitasServlet</servlet-name>
        <servlet-class>com.labclinico.omega.servlet.CitasServlet</servlet-class>
        <servlet-name>AnalisisServlet</servlet-name>
        <servlet-class>com.labclinico.omega.servlet.AnalisisServlet</servlet-class>
        <servlet-name>MuestraServlet</servlet-name>
        <servlet-class>com.labclinico.omega.servlet.MuestraServlet</servlet-class>
        <servlet-name>RecursoDigitalServlet</servlet-name>
        <servlet-class>com.labclinico.omega.servlet.RecursoDigitalServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PatientServlet</servlet-name>
        <url-pattern>/patients/*</url-pattern>
        <servlet-name>DoctoresServlet</servlet-name>
        <url-pattern>/doctores/*</url-pattern>
        <servlet-name>CitasServlet</servlet-name>
        <url-pattern>/citas/*</url-pattern>
        <servlet-name>AnalisisServlet</servlet-name>
        <url-pattern>/analisis/*</url-pattern>
        <servlet-name>MuestraServlet</servlet-name>
        <url-pattern>/muestras/*</url-pattern>
        <servlet-name>RecursoDigitalServlet</servlet-name>
        <url-pattern>/recursoDigital/*</url-pattern>
    </servlet-mapping>

</web-app>